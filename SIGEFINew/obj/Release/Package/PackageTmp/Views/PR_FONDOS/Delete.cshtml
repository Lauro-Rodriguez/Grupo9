@model SIGEFINew.Models.Presupuesto.PR_FONDOS

@{
    ViewBag.Title = "Delete";
}
<div class="col-md-8">
    <div class="box box-warning">
        <div class="box-header with-border">
            <i class="glyphicon glyphicon-remove"></i>
            <h3 class="box-title">Borrar Fuente de Financiamiento</h3>
        </div>
        <div class="box-body">
            <h4>Está seguro de borrar este registro?</h4>
            <div>
                <dl class="dl-horizontal">
                    <dt>
                        @Html.DisplayNameFor(model => model.ID_FONDO)
                    </dt>

                    <dd>
                        @Html.DisplayFor(model => model.ID_FONDO)
                    </dd>

                    <dt>
                        @Html.DisplayNameFor(model => model.DESCRIPCION)
                    </dt>

                    <dd>
                        @Html.DisplayFor(model => model.DESCRIPCION)
                    </dd>

                </dl>

                @using (Html.BeginForm())
                {
                    @Html.AntiForgeryToken()

                    <div class="form-actions no-color">
                        <input type="submit" value="Borrar" class="btn btn-danger" /> |
                        @Html.ActionLink("Regresar a la Lista", "Index")
                    </div>
                }
            </div>
        </div>
    </div>
</div>

