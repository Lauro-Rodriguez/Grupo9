@model SIGEFINew.Models.Presupuesto.PR_SOLICDISPPRESUP

@{
    ViewBag.Title = "Anular";
}

<div class="col-md-8">
    <div class="box box-warning">
        <div class="box-header with-border">
            <i class="glyphicon glyphicon-remove-sign"></i>
            <h3 class="box-title">Borrar Grupo Presupuestario</h3>
        </div>
        <div class="box-body">
            @if (@ViewBag.MsgError != null)
            {
                <div class="col-md-12">
                    <div class="alert alert-danger alert-dismissable">
                        <button type="button" class="close" data-dismiss="alert" aria-hidden="true">x</button>
                        <h4><i class="icon alert-danger"></i>Mensaje</h4>
                        @ViewBag.MsgError
                    </div>
                </div>
            }
            <h4>Está seguro de Anular este registro?</h4>
            <dl class="dl-horizontal">
                <dt>
                    N° Solicitud
                </dt>
                <dd>
                    @Html.DisplayFor(model => model.NUM_SOLIC)
                </dd>

                <dt>
                    @Html.DisplayNameFor(model => model.DETALLE)
                </dt>

                <dd>
                    @Html.DisplayFor(model => model.DETALLE)
                </dd>

                <dt>
                    @Html.DisplayNameFor(model => model.VALOR)
                </dt>

                <dd>
                    @Html.DisplayFor(model => model.VALOR)
                </dd>

            </dl>

            @using (Html.BeginForm())
            {
                @Html.AntiForgeryToken()

                <div class="form-actions no-color">
                    <input type="submit" value="Anular" class="btn btn-danger" /> |
                    @Html.ActionLink("Regresar", "Index")
                </div>
            }
        </div>
    </div>
</div>
