@model IEnumerable<SIGEFINew.Models.Contabilidad.CO_CTASPPAGAR>

@{
    ViewBag.Title = "Index";
}

<div class="form-group">
    <div class="row">
        <div class="col-md-1">
            <button id="buttonnew" type="submit" onclick="@("location.href='"+ Url.Action("Create","CO_CTASPPAGAR")+ "'")" class="btn btn-primary">
                <span class="fa fa-plus"></span> Nuevo
            </button>
        </div>
        @*<div class="col-md-1">
            <button type="submit" onclick="@("location.href='"+ Url.Action("Print","PartIngresoes")+ "'")" class="btn btn-primary">
                <span class="glyphicon glyphicon-print"></span> Imprimir
            </button>
        </div>*@
        <div class="col-md-6 col-lg-offset-5">
            @using (Html.BeginForm())
            {
                <div class="input-group">
                    @Html.Editor("SearchString", ViewBag.CurrentFilter as string, new { htmlAttributes = new { @class = "form-control", placeholder = "Buscar" } })
                    <span class="input-group-btn">
                        <button type="submit" class="btn btn-info">
                            <span class="glyphicon glyphicon-search"></span>
                        </button>
                    </span>

                </div><!-- /input-group -->
            }
        </div>

    </div>

</div>
<div class="box box-primary">
    <div class="box-header">
        <i class="ion ion-clipboard"></i>
        <h3 class="box-title">Cuentas por Pagar</h3>
    </div>
    <div class="box-body">
        <table class="table table-striped table-bordered table-hover table-condensed">
            <thead>
                <tr>
                    <th style="width:10%">
                        @Html.DisplayNameFor(model => model.FECHA_TRANSAC)
                    </th>
                    <th style="width:20%">
                        @Html.DisplayNameFor(model => model.Proveedores.NOMBRE)
                    </th>
                    <th style="width:10%">
                        @Html.DisplayNameFor(model => model.Proveedores.NUMCEDRUC_PROV)
                    </th>
                    <th style="width:30%">
                        @Html.DisplayNameFor(model => model.DETALLE)
                    </th>
                    <th style="width:10%">
                        @Html.DisplayNameFor(model => model.NUM_COMPROMISO)
                    </th>
                    <th style="width:10%">
                        @Html.DisplayNameFor(model => model.NUM_TRANSAC)
                    </th>
                  
                    <th style="width:10%"></th>
                </tr>
            </thead>
            

            @foreach (var item in Model)
            {
                <tr>
                    <td style="width:10%">
                        @Html.DisplayFor(modelItem => item.FECHA_TRANSAC)
                    </td>
                    <td style="width:20%">
                        @Html.DisplayFor(modelItem => item.Proveedores.NOMBRE)
                    </td>
                    <td style="width:10%">
                        @Html.DisplayFor(modelItem => item.Proveedores.NUMCEDRUC_PROV)
                    </td>
                    <td style="width:30%">
                        @Html.DisplayFor(modelItem => item.DETALLE)
                    </td>
                    <td style="width:10%">
                        @Html.DisplayFor(modelItem => item.NUM_COMPROMISO)
                    <td style="width:10%">
                        @Html.DisplayFor(modelItem => item.NUM_TRANSAC)
                    </td>
                    
                    <td style="width:10%">
                        @Html.ActionLink("Edit", "Edit", new { /* id=item.PrimaryKey */ }) |
                        @Html.ActionLink("Details", "Details", new { /* id=item.PrimaryKey */ }) |
                        @Html.ActionLink("Delete", "Delete", new { /* id=item.PrimaryKey */ })
                    </td>
                </tr>
            }

        </table>
    </div>
</div>


