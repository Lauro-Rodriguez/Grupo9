@model IEnumerable<SIGEFINew.ViewModels.POA_DetalProys>
@*@model IEnumerable<SIGEFINew.Models.POA.POA_PLANOBJETIVOS>*@
@{
    ViewBag.Title = "Objetivos";
}

<div class="form-group">
    <div class="row">
        <div class="col-md-1">
            <button id="buttonnew" type="submit" onclick="@("location.href='"+ Url.Action("Create","POA_PLANOBJETIVOS", new {@CodPlan=ViewBag.CodPlan })+ "'")" class="btn btn-primary">
                <span class="fa fa-plus"></span> Nuevo
            </button>
        </div>
        <div class="col-md-1">
            <button type="submit" onclick="@("location.href='"+ Url.Action("Print","POA_PLANOBJETIVOS", new {@CodPlan=ViewBag.CodPlan })+ "'")" class="btn btn-primary">
                <span class="glyphicon glyphicon-print"></span> Imprimir
            </button>
        </div>
        <div class="col-md-5 col-lg-offset-5">
            @using (Html.BeginForm())
            {
                <div class="input-group">
                    @Html.Editor("SearchString", ViewBag.CurrentFilter as string, new { htmlAttributes = new { @class = "form-control", placeholder = "Buscar por Nombre" } })
                    <span class="input-group-btn">
                        <button type="submit" class="btn btn-info">
                            <span class="glyphicon glyphicon-search"></span>
                        </button>
                    </span>

                </div><!-- /input-group -->
            }
        </div>

    </div>

</div>
<div class="box box-primary">
    <div class="box-header">
        <i class="ion ion-clipboard"></i>
        <h3 class="box-title">Proyectos <span style="font-size:25px; text-align:right; color:darkgreen">Total : $ @ViewBag.Total</span></h3>
         
    </div>
    <div class="box-body">
        <div class="table-responsive text-nowrap">
            <table class="table table-striped table-bordered table-hover table-condensed">
                <thead>
                    <tr>
                        <th style="width:5%">
                            Código
                        </th>
                        <th style="width:20%">
                            Responsable
                        </th>

                        @*<th>
                                @Html.DisplayNameFor(model => model.FECHA_INICIAL)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.FECHA_FINAL)
                            </th>*@
                        <th style="width:20%">
                            @Html.DisplayNameFor(model => model.DESC_OBJETIVO)
                        </th>
                        <th style="width:20%">
                            @Html.DisplayNameFor(model => model.OBS_OBJETIVO)
                        </th>
                        <th style="width:5%">
                            @Html.DisplayNameFor(model => model.ESTADO)
                        </th>
                        <th style="width:5%">
                            @Html.DisplayNameFor(model => model.ES_GAPR)
                        </th>
                        <th style="width:8%">
                            Total
                        </th>
                        <th style="width:10%"></th>
                    </tr>
                </thead>


                @foreach (var item in Model)
                {
                    <tr>
                        <td style="width:5%">
                            @Html.DisplayFor(modelItem => item.COD_PLANOBJTVO)
                        </td>
                        <td style="width:20%">
                            @Html.DisplayFor(modelItem => item.APELLIDOSNOMBRES)
                        </td>

                        @*<td>
                                @Html.DisplayFor(modelItem => item.FECHA_INICIAL)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.FECHA_FINAL)
                            </td>*@
                        <td style="width:20%">
                            @Html.DisplayFor(modelItem => item.DESC_OBJETIVO)
                        </td>
                        <td style="width:20%">
                            @Html.DisplayFor(modelItem => item.OBS_OBJETIVO)
                        </td>
                        <td style="width:5%">
                            @Html.DisplayFor(modelItem => item.ESTADO)
                        </td>
                        <td style="width:5%">
                            @Html.DisplayFor(modelItem => item.ES_GAPR)
                        </td>
                        <td style="width:8%;text-align:right">
                            @Html.DisplayFor(modelItem => item.TOTAL)
                        </td>
                        <td style="width:10%">
                            @Html.ActionLink(" ", "Create", new { CodPlan = item.COD_PLAN, CodPlanObj = item.COD_PLANOBJTVO }, new { @class = "btn btn-primary glyphicon glyphicon-edit btn-xs", data_toggle = "tooltip", title = "Editar" })
                            @Html.ActionLink(" ", "Details", new { id = item.COD_PLAN, id2 = item.ORG_CODIGO, id3 = item.COD_PLANOBJTVO }, new { @class = "btn btn-success glyphicon glyphicon-tasks btn-xs", data_toggle = "tooltip", title = "Indicadores" }) |
                            @Html.ActionLink(" ", "Recurso", new { id = item.COD_PLAN, id2 = item.ORG_CODIGO, id3 = item.COD_PLANOBJTVO }, new { @class = "btn btn-success glyphicon glyphicon-equalizer btn-xs", data_toggle = "tooltip", title = "Recursos" }) |
                            @*@Html.ActionLink(" ", "Fases", new { id = item.COD_PLAN, id2 = item.ORG_CODIGO, id3 = item.COD_PLANOBJTVO }, new { @class = "btn btn-success glyphicon glyphicon-cog btn-xs", data_toggle = "tooltip", title = "Fases" }) |*@
                            @Html.ActionLink(" ", "Delete", new { id = item.COD_PLAN, id2 = item.ORG_CODIGO, id3 = item.COD_PLANOBJTVO }, new { @class = "btn btn-danger glyphicon glyphicon-trash btn-xs", data_toggle = "tooltip", title = "Borrar" })
                        </td>
                    </tr>
                }

            </table>
            @*<div class="table-responsive text-nowrap">
                    <table class="table table-striped table-bordered table-hover table-condensed">
                        <thead>
                            <tr>
                                <th>Month</th>
                                <th>Savings</th>
                            </tr>
                        </thead>
                        <tbody>

                            <tr>
                                <th scope="row">1</th>
                                <td>January</td>
                                <td>$100</td>
                            </tr>
                            <tr>
                                <th scope="row">2</th>
                                <td>February</td>
                                <td>$80</td>
                            </tr>
                        </tbody>
                        <tfoot>
                            <tr>

                                <td>Sum</td>
                                <td>$180</td>
                            </tr>
                        </tfoot>
                    </table>
                </div>*@
        </div>
    </div>
</div>
