}

<div class="form-group">
    <div class="row">
        @if (ViewBag.EstPY == "GE" || ViewBag.EstPY == "DS" || ViewBag.EstPY == "DA")
        {
            <div class="col-md-1">
                <button id="buttonnew" type="submit" onclick="@("location.href='"+ Url.Action("Create","POA_OBJTIVORECURSOS",new {@id=ViewBag.CodPlan,@id3=ViewBag.CodPlanObj })+ "'")" class="btn btn-primary">
                    <span class="fa fa-plus"></span> Nuevo
