@model IEnumerable<SIGEFINew.Models.DOCUMSPDFS>
<div class="form-group">
    <div class="row">
        <div class="col-md-1">
            <button id="buttonnew" type="submit" onclick="@("location.href='"+ Url.Action("Create","POA_FASESPROYS",new {@CodPlan=ViewBag.CodPlan,@CodPObtvo=ViewBag.CodPlanObj })+ "'")" class="btn btn-primary">
                <span class="fa fa-plus"></span> Nuevo
            </button>
        </div>
        @*<div class="col-md-1">
                <button type="submit" onclick="@("location.href='"+ Url.Action("Print","PartIngresoes")+ "'")" class="btn btn-primary">
                    <span class="glyphicon glyphicon-print"></span> Imprimir
                </button>
            </div>*@
        <div class="col-md-5 col-lg-offset-6">
            @using (Html.BeginForm())
            {
                <div class="input-group">
                    @Html.Editor("SearchString", ViewBag.CurrentFilter as string, new { htmlAttributes = new { @class = "form-control", placeholder = "Buscar por Nombre" } })
                    <span class="input-group-btn">
                        <button type="submit" class="btn btn-info">
                            <span class="glyphicon glyphicon-search"></span>
                        </button>
                    </span>

                </div><!-- /input-group -->
            }
        </div>

    </div>

</div>
<div class="box box-primary">
    <div class="box-header">
        <i class="ion ion-clipboard"></i>
        <h3 class="box-title">Documentos Subidos</h3>
    </div>
    <div class="box-body">
        <table class="table table-striped table-bordered table-hover table-condensed">
            <thead>
                <tr>
                    <th style="width:90%">
                        @Html.DisplayNameFor(model => model.NOM_ARCHIVO)
                    </th>

                    <th style="width:10%"></th>
                </tr>
            </thead>


            @foreach (var item in Model)
            {
                <tr>

                    <td style="width:12%">
                        @Html.DisplayFor(modelItem => item.NOM_ARCHIVO)
                    </td>


                    <td style="width:10%">
                        @*@Html.ActionLink(" ", "Ver", new { CodPlan = item.COD_PLAN, CodPObtvo = item.COD_PLANOBJTVO, CodFase = item.COD_FASE }, new { @class = "btn btn-information glyphicon glyphicon-trash btn-xs", data_toggle = "tooltip", title = "Ver" })*@

                    </td>
                </tr>
            }

        </table>
    </div>
</div>

