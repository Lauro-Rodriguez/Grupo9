@model IEnumerable<SIGEFINew.Models.Contabilidad.CO_CUENBANCOS>

@{
    ViewBag.Title = "Cuentas Banc.";
}

<div class="form-group">
    <div class="row">
        <div class="col-md-1">
            <button id="buttonnew" type="submit" onclick="@("location.href='"+ Url.Action("Create","CO_CUENBANCOS")+ "'")" class="btn btn-primary">
                <span class="fa fa-plus"></span> Nuevo
            </button>
        </div>
        @*<div class="col-md-1">
            <button type="submit" onclick="@("location.href='"+ Url.Action("Print","PartIngresoes")+ "'")" class="btn btn-primary">
                <span class="glyphicon glyphicon-print"></span> Imprimir
            </button>
        </div>*@
        <div class="col-md-6 col-lg-offset-5">
            @using (Html.BeginForm())
            {
                <div class="input-group">
                    @Html.Editor("SearchString", ViewBag.CurrentFilter as string, new { htmlAttributes = new { @class = "form-control", placeholder = "Buscar por Nombre" } })
                    <span class="input-group-btn">
                        <button type="submit" class="btn btn-info">
                            <span class="glyphicon glyphicon-search"></span>
                        </button>
                    </span>

                </div><!-- /input-group -->
            }
        </div>

    </div>

</div>
<div class="box box-primary">
    <div class="box-header">
        <i class="ion ion-clipboard"></i>
        <h3 class="box-title">Cuentas Bancarias</h3>
    </div>
    <div class="box-body">
        <table class="table table-striped table-bordered table-hover table-condensed">
            <thead>
                <tr>
                    <th style="width:5%">
                        @Html.DisplayNameFor(model => model.CODIGO_CUENTA)
                    </th>
                    <th style="width:20%">
                        Banco
                    </th>
                    <th style="width:40%">
                        @Html.DisplayNameFor(model => model.DESCRIPCION)
                    </th>
                    <th style="width:10%">
                        @Html.DisplayNameFor(model => model.CUENTA_CORRIENTE)
                    </th>
                    <th style="width:10%">
                        @Html.DisplayNameFor(model => model.CODIGO_CG)
                    </th>
                    <th style="width:5%">
                        @Html.DisplayNameFor(model => model.CONCILIA)
                    </th>
                    @*<th style="width:10%">
                        @Html.DisplayNameFor(model => model.CTA_DEBITO)
                    </th>
                    <th style="width:10%">
                        @Html.DisplayNameFor(model => model.CTA_CREDITO)
                    </th>*@

                    <th style="width:10%"></th>
                </tr>
            </thead>

            @foreach (var item in Model)
            {
                <tr>
                    <td style="width:5%">
                        @Html.DisplayFor(modelItem => item.CODIGO_CUENTA)
                    </td>
                    <td style="width:20%">
                        @Html.DisplayFor(modelItem => item.BANCOS.DESCRIPCION)
                    </td>
                    <td style="width:40%">
                        @Html.DisplayFor(modelItem => item.DESCRIPCION)
                    </td>
                    <td style="width:10%">
                        @Html.DisplayFor(modelItem => item.CUENTA_CORRIENTE)
                    </td>

                    <td style="width:10%">
                        @Html.DisplayFor(modelItem => item.CODIGO_CG)
                    </td>

                    <td style="width:5%">
                        @Html.DisplayFor(modelItem => item.CONCILIA)
                    </td>
                    @*<td style="width:10%">
                        @Html.DisplayFor(modelItem => item.CTA_DEBITO)
                    </td>
                    <td style="width:10%">
                        @Html.DisplayFor(modelItem => item.CTA_CREDITO)
                    </td>*@

                    <td style="width:10%">
                        @Html.ActionLink(" ", "Create", new { id = item.CODIGO_CUENTA }, new { @class = "btn btn-primary glyphicon glyphicon-edit btn-xs", data_toggle = "tooltip", title = "Editar" }) |
                        @Html.ActionLink(" ", "Delete", new { id = item.CODIGO_CUENTA }, new { @class = "btn btn-danger glyphicon glyphicon-trash btn-xs", data_toggle = "tooltip", title = "Eliminar" })
                    </td>
                </tr>
            }

        </table>
    </div>
</div>


