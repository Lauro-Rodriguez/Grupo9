@model IEnumerable<SIGEFINew.Models.POA.POA_INDICADOROBJ>

@{
    ViewBag.Title = "Indicadores";
}

<div class="form-group">
    <div class="row">
        @if (ViewBag.EstPY == "GE" || ViewBag.EstPY == "DS" || ViewBag.EstPY == "DA")
        {
            <div class="col-md-1">
                <button id="buttonnew" type="submit" onclick="@("location.href='"+ Url.Action("Edit","POA_INDICADOROBJ",new {@CodPlan=ViewBag.Cod_Plan,@CodPlObtvo=ViewBag.CodPlanObj })+ "'")" class="btn btn-primary">
                    <span class="fa fa-plus"></span> Nuevo
                </button>
            </div>
        }

        @*<div class="col-md-1">
            <button type="submit" onclick="@("location.href='"+ Url.Action("Print","POA_INDICADOROBJ")+ "'")" class="btn btn-primary">
                <span class="glyphicon glyphicon-print"></span> Imprimir
            </button>
        </div>*@
        <div class="col-md-6 col-lg-offset-5">
            @using (Html.BeginForm())
            {
                <div class="input-group">
                    @Html.Editor("SearchString", ViewBag.CurrentFilter as string, new { htmlAttributes = new { @class = "form-control", placeholder = "Buscar por Nombre" } })
                    <span class="input-group-btn">
                        <button type="submit" class="btn btn-info">
                            <span class="glyphicon glyphicon-search"></span>
                        </button>
                    </span>

                </div><!-- /input-group -->
            }
        </div>

    </div>

</div>
<div class="box box-primary">
    <div class="box-header">
        <i class="ion ion-clipboard"></i>
        <h3 class="box-title">Indicadores</h3>
    </div>
    <div class="box-body">
        <table class="table table-striped table-bordered table-hover table-condensed">
            <thead>
                <tr>
                    <th style="width:5%">
                        @Html.DisplayNameFor(model => model.POA_PLANOBJETIVOS.COD_OE_PLANIF)
                    </th>
                    <th style="width:10%">
                        Tipo
                    </th>
                    <th style="width:5%">
                        Código
                    </th>
                    <th style="width:5%">
                        @Html.DisplayNameFor(model => model.VAL_ENERO)
                    </th>
                    <th style="width:5%">
                        @Html.DisplayNameFor(model => model.VAL_FEBRERO)
                    </th>
                    <th style="width:5%">
                        @Html.DisplayNameFor(model => model.VAL_MARZO)
                    </th>
                    <th style="width:5%">
                        @Html.DisplayNameFor(model => model.VAL_ABRIL)
                    </th>
                    <th style="width:5%">
                        @Html.DisplayNameFor(model => model.VAL_MAYO)
                    </th>
                    <th style="width:5%">
                        @Html.DisplayNameFor(model => model.VAL_JUNIO)
                    </th>
                    <th style="width:5%">
                        @Html.DisplayNameFor(model => model.VAL_JULIO)
                    </th>
                    <th style="width:5%">
                        @Html.DisplayNameFor(model => model.VAL_AGOSTO)
                    </th>
                    <th style="width:7%">
                        @Html.DisplayNameFor(model => model.VAL_SEPTIEMBRE)
                    </th>
                    <th style="width:7%">
                        @Html.DisplayNameFor(model => model.VAL_OCTUBRE)
                    </th>
                    <th style="width:7%">
                        @Html.DisplayNameFor(model => model.VAL_NOVIEMBRE)
                    </th>
                    <th style="width:7%">
                        @Html.DisplayNameFor(model => model.VAL_DICIEMBRE)
                    </th>
                    <th style="width:7%">
                        @Html.DisplayNameFor(model => model.TOTAL)
                    </th>
                    <th style="width:5%"></th>
                </tr>
            </thead>

            @foreach (var item in Model)
            {
                <tr>
                    <td style="width:5%">
                        @Html.DisplayFor(modelItem => item.POA_PLANOBJETIVOS.COD_OE_PLANIF)
                    </td>
                    <td style="width:10%">
                        @Html.DisplayFor(modelItem => item.TIPOOBJ)
                    </td>
                    <td style="width:5%">
                        @Html.DisplayFor(modelItem => item.COD_INDICADOROJ)
                    </td>
                    <td style="width:5%">
                        @Html.DisplayFor(modelItem => item.VAL_ENERO)
                    </td>
                    <td style="width:5%">
                        @Html.DisplayFor(modelItem => item.VAL_FEBRERO)
                    </td>
                    <td style="width:5%">
                        @Html.DisplayFor(modelItem => item.VAL_MARZO)
                    </td>
                    <td style="width:5%">
                        @Html.DisplayFor(modelItem => item.VAL_ABRIL)
                    </td>
                    <td style="width:5%">
                        @Html.DisplayFor(modelItem => item.VAL_MAYO)
                    </td>
                    <td style="width:5%">
                        @Html.DisplayFor(modelItem => item.VAL_JUNIO)
                    </td>
                    <td style="width:5%">
                        @Html.DisplayFor(modelItem => item.VAL_JULIO)
                    </td>
                    <td style="width:5%">
                        @Html.DisplayFor(modelItem => item.VAL_AGOSTO)
                    </td>
                    <td style="width:7%">
                        @Html.DisplayFor(modelItem => item.VAL_SEPTIEMBRE)
                    </td>
                    <td style="width:7%">
                        @Html.DisplayFor(modelItem => item.VAL_OCTUBRE)
                    </td>
                    <td style="width:7%">
                        @Html.DisplayFor(modelItem => item.VAL_NOVIEMBRE)
                    </td>
                    <td style="width:7%">
                        @Html.DisplayFor(modelItem => item.VAL_DICIEMBRE)
                    </td>
                    <td style="width:7%">
                        @Html.DisplayFor(modelItem => item.TOTAL)
                    </td>
                    <td style="width:5%">
                        @Html.ActionLink(" ", "Edit", new { id = item.COD_PLAN, id2 = item.ORG_CODIGO, id3 = item.COD_PLANOBJTVO, id4 = item.COD_INDICADOROJ,CodPlan=item.COD_PLAN, CodPlObtvo=item.COD_PLANOBJTVO }, new { @class = "btn btn-primary glyphicon glyphicon-edit btn-xs", data_toggle = "tooltip", title = "Editar" })
                        @*@Html.ActionLink("Editar", "Edit", new { id = item.COD_PLAN, id2 = item.ORG_CODIGO, id3 = item.COD_PLANOBJTVO, id4 = item.COD_INDICADOROJ }) |*@
                        @*@Html.ActionLink("Details", "Details", new { /* id=item.PrimaryKey */ }) |
        @Html.ActionLink("Delete", "Delete", new { /* id=item.PrimaryKey */ })*@
                    </td>
                </tr>
            }

        </table>
    </div>
</div>

