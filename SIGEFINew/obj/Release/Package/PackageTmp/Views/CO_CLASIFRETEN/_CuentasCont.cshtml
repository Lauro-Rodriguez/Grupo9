@model IEnumerable<SIGEFINew.Models.Contabilidad.CO_RELARETCONT>

<div>
    <table class="table table-striped table-bordered table-hover table-condensed">
        <thead>
            <tr>
                <th style="width:10%">
                    Código
                </th>
                <th style="width:80%">
                    Descripción
                </th>
                <th style="width:10%">Opciones</th>
            </tr>
        </thead>


        @foreach (var item in Model)
        {
            <tr>
                <td style="width:10%">
                    @Html.DisplayFor(modelitem => item.CODIGO_CG, new { htmlAttributes = new { @class = "form-control" } })
                </td>
                <td style="width:80%">
                    @Html.DisplayFor(modelitem => item.CO_CUENTASCONT.NOMBRE_CG, new { htmlAttributes = new { @class = "form-control" } })
                </td>
                <td style="width:10%">
                    @Html.ActionLink(" ", "BorraCta", new { id = item.CODIGO_RET, id2 = item.CODIGO_CG }, new { @class = "btn btn-danger glyphicon glyphicon-trash btn-xs", data_toggle = "tooltip", title = "Eliminar" })
                    @*<button type="submit" onclick="@("location.href='"+ Url.Action("Print","PartIngresoes")+ "'")" class="btn btn-danger btn-xs" id="Delete"  data_toggle = "tooltip" title = "Eliminar">
                        <span class="glyphicon glyphicon-trash"></span>
                    </button>*@
                </td>
            </tr>

        }

    </table>
</div>

<script>
    $(document).ready(function () {
        $("#Delete").click(function () {
            alert("Hola Mundo con jQuery!");
        });
    });
</script>    