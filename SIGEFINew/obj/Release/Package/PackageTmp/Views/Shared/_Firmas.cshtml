@model IEnumerable<SIGEFINew.Models.FIRMAS>

<table class="table table-striped table-bordered table-hover table-condensed">
    <tr>
        <th>
            Nombre
        </th>
        <th>
            Cargo
        </th>
        <th>
            Tipo
        </th>
        <th></th>
    </tr>
    @*@{
            List<SIGEFINew.ViewModels.AsignarDetDispPresup> Recursos=ViewBag.Recursos;
        }*@
    @foreach (var item in Model)
    {
<tr>
    <td>
        @Html.DisplayFor(modelitem => item.NOMBRE, new { htmlAttributes = new { @class = "form-control" } })
    </td>
    <td>
        @Html.DisplayFor(modelitem => item.CARGO, new { htmlAttributes = new { @class = "form-control" } })
    </td>
    <td>
        @Html.DisplayFor(modelitem => item.TIPO, new { htmlAttributes = new { @class = "form-control" } })
    </td>
    <td>
        <input type="button" title="Eliminar" value="Eliminar" onclick="location.href='@Url.Action("Delete", "PR_SOLICDISPPRESUP", new { id = item.CODIGO_DOC,id2=item.ORG_CODIGO,id3=item.NUM_SOLIC})'" class="btn btn-danger btn-xs" />
    </td>
</tr>
    }


